import React, { Component } from "react";

class DeleteButton extends Component {
  state = {

  };
  render() {
    console.log(this.props.Movie);
    return (
      <button
        type="Submit"
        className="btn btn-danger"
        onClick={()=>this.props.onDelete(this.props.Movie)}
      >
        Delete
      </button>
    );
  }
}

export default DeleteButton;
